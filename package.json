{
  "name": "@adddog/monorepo-consistency",
  "version": "0.0.8",
  "type": "module",
  "bin": {
    "mono": "./dist/cli.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "types": "tsc -p tsconfig.typecheck.json",
    "dev": "tsx src/cli.ts",
    "release": "bumpp --yes --no-push --no-tag --release patch",
    "generate-schema": "tsx scripts/generate-schema.ts",
    "prepublishOnly": "pnpm run test && pnpm run lint && pnpm run types && pnpm run test && pnpm run knip && pnpm run build",
    "prepack": "pnpm run prepublishOnly",
    "build:watch": "unbuild --watch",
    "test": "echo '⚠️⚠️⚠️no test vitest --watch=false'",
    "knip": "knip"
  },
  "dependencies": {
    "@adddog/zod-to-json-schema": "^0.0.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "fast-glob": "^3.3.3",
    "get-tsconfig": "^4.13.6",
    "lodash": "^4.17.23",
    "minimatch": "^10.1.2",
    "ora": "^9.3.0",
    "yaml": "^2.8.2",
    "zod": "^4.3.6",
    "zx": "^8.8.5"
  },
  "devDependencies": {
    "@adddog/build-configs": "^0.0.5",
    "@types/lodash": "^4.17.23",
    "@types/node": "^25.2.1",
    "@vitest/coverage-v8": "^4.0.18",
    "bumpp": "^10.4.1",
    "eslint": "^9.39.2",
    "knip": "^5.83.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "@vue/compiler-sfc": "^3.5.22"
  },
  "engines": {
    "node": ">= 22",
    "pnpm": ">= 10",
    "npm": ">= 11"
  },
  "publishConfig": {
    "access": "public"
  }
}
