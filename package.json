{
  "name": "@adddog/monorepo-consistency",
  "version": "0.0.11",
  "type": "module",
  "bin": {
    "mono": "./dist/cli.mjs"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "types": "tsc -p tsconfig.typecheck.json",
    "dev": "tsx src/cli.ts",
    "release": "bumpp --yes --no-push --no-tag --release patch",
    "generate-schema": "tsx scripts/generate-schema.ts",
    "prepublishOnly": "pnpm run test && pnpm run lint && pnpm run types && pnpm run test && pnpm run knip && pnpm run build",
    "prepack": "pnpm run prepublishOnly",
    "build:watch": "unbuild --watch",
    "test": "vitest --watch=false",
    "knip": "knip",
    "version:patch": "npm version patch --no-git-tag-version"
  },
  "dependencies": {
    "@adddog/zod-to-json-schema": "workspace:*",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "fast-glob": "^3.3.3",
    "lodash": "^4.17.23",
    "minimatch": "^10.2.2",
    "ora": "^9.3.0",
    "yaml": "^2.8.2",
    "zod": "^4.3.6",
    "zx": "^8.8.5"
  },
  "devDependencies": {
    "@adddog/build-configs": "^0.0.6",
    "@types/lodash": "^4.17.24",
    "@types/node": "^25.3.0",
    "@vitest/coverage-v8": "^4.0.18",
    "bumpp": "^10.4.1",
    "eslint": "^10.0.2",
    "knip": "^5.85.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "@vue/compiler-sfc": "^3.5.22"
  },
  "engines": {
    "node": ">= 22",
    "pnpm": ">= 10",
    "npm": ">= 11"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.mts",
        "import": "./dist/index.mjs"
      }
    },
    "main": "./dist/index.mjs",
    "types": "./dist/index.d.mts"
  }
}
